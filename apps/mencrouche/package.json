{
	"name": "@mencrouche/app",
	"version": "1.0.1",
	"author": "FOBshippingpoint",
	"dependencies": {
		"@mencrouche/types": "*",
		"@mencrouche/apocalypse": "*",
		"@mencrouche/dataset": "*",
		"@mencrouche/dollars": "*",
		"@mencrouche/n81i": "*"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"@types/chrome": "^0.1.1",
		"@types/firefox-webext-browser": "^120.0.4",
		"@types/webextension-polyfill": "^0.12.3",
		"@types/youtube": "^0.1.2",
		"dompurify": "^3.2.6",
		"happy-dom": "^18.0.1",
		"kikey": "^3.1.0",
		"marked": "^16.1.1",
		"open-props": "2.0.0-beta.5",
		"prismjs": "^1.30.0",
		"quill": "^2.0.3",
		"typedoc": "^0.28.9",
		"typescript": "^5.9.2",
		"vite": "^7.0.6",
		"vitest": "^3.2.4",
		"web-ext": "^8.9.0"
	},
	"description": "A highly customizable web extension for new tab. Bring your own content to build your web experience.",
	"license": "AGPL-3.0",
	"scripts": {
		"test": "vitest",
		"format": "biome format --write",
		"site:dev": "vite --open",
		"site:build": "vite build",
		"ext:firefox": "web-ext run --source-dir dist --target=firefox-desktop",
		"ext:chrome": "web-ext run --source-dir dist --target=chromium",
		"ext:lint": "web-ext lint --source-dir dist",
		"ext:pack": "web-ext build --overwrite-dest --source-dir dist"
	},
	"type": "module"
}
